{
  "name" : "Re-skinning Group - Play App Sortering",
  "version" : "0.1",
  "author": "Mohammed Cha",
  "description" : "Ability to sort the Play store Apps",
  "background" : {
		"scripts": ["background.js"]
  },
  "permissions": [
	"contextMenus",
	"https://play.google.com/store/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://play.google.com/store/*"],
      "js": ["jquery-1.11.0.min.js","content.js"]
    }
  ],
  
	"icons": 
	{"16": "icon16.png",
	 "48": "icon48.png",
	 "64": "icon64.png",
	 "128": "icon128.png"
	},

  
  "manifest_version": 2
}
